{
    "manifest_version": 3,
    "name": "Brightspace Linkternal",
    "version": "0.1.1",
    "description": "Make external links in assessment submissions open in a new tab.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["http://*/*", "https://*/*"]
        }
    ],
    "author": "Stephen Peasley",  
    "homepage_url": "https://github.com/speasley/brightspace-linkternal-chrome"
  }
